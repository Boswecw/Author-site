		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@sveltejs/vite-plugin-svelte-inspector": "^5.0.1",
		"@tailwindcss/typography": "^0.5.10",
		"@types/mongodb": "^4.0.6",
		"@types/node": "^20.10.0",
		"autoprefixer": "^10.4.16",
		"postcss": "^8.4.32",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"svelte": "^5.38.6",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.0",
		"typescript": "^5.0.0",
