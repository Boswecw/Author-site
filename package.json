	  "autoprefixer": "^10.4.16",
	  "postcss": "^8.4.32",
	  "prettier": "^3.1.0",
	  "prettier-plugin-svelte": "^3.1.0",
	  "svelte": "^4.2.7",
	  "svelte-check": "^3.6.0",
	  "tailwindcss": "^3.3.6",
	  "tslib": "^2.6.0",
	  "typescript": "^5.0.0",
